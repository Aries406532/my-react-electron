{
    "compilerOptions": {
      "target": "es2016",
      "module": "commonjs",    
      "outDir": "dist",
      "strict": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "baseUrl": ".",
      "paths": {
      },
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": [
      "./main/**/*"
    ]
  }
  